        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# note that the N is set in the first routine&amp;#10;import random&amp;#10;sequence = []&amp;#10;all_stimuli = ['B', 'C', 'D', 'F', 'G', 'H', 'J', 'K']" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="import * as random from 'random';&amp;#10;sequence = [];&amp;#10;all_stimuli = [&quot;B&quot;, &quot;C&quot;, &quot;D&quot;, &quot;F&quot;, &quot;G&quot;, &quot;H&quot;, &quot;J&quot;, &quot;K&quot;];&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="if (((sequence.length &gt; n) &amp;&amp; (Math.random.random() &lt; 0.5))) {&amp;#10;    curr_stim = sequence.slice((- n))[0];&amp;#10;    is_target = true;&amp;#10;} else {&amp;#10;    curr_stim = Math.random.choice(all_stimuli);&amp;#10;    if (((sequence.length &gt; n) &amp;&amp; (curr_stim === sequence.slice((- n))[0]))) {&amp;#10;        remaining = function () {&amp;#10;    var _pj_a = [], _pj_b = all_stimuli;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var l = _pj_b[_pj_c];&amp;#10;        if ((l !== sequence.slice((- n))[0])) {&amp;#10;            _pj_a.push(l);&amp;#10;        }&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;        curr_stim = Math.random.choice(remaining);&amp;#10;    }&amp;#10;    is_target = false;&amp;#10;}&amp;#10;sequence.push(curr_stim);&amp;#10;psychoJS.experiment.addData(&quot;stim&quot;, curr_stim);&amp;#10;psychoJS.experiment.addData(&quot;is_target&quot;, is_target);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# do a target with 50% probability&amp;#10;if len(sequence) &gt; n and random.random() &lt; 0.5:&amp;#10;    curr_stim = sequence[-n]&amp;#10;    is_target = True&amp;#10;else:&amp;#10;    curr_stim = random.choice(all_stimuli)&amp;#10;    # make sure to pick a different letter than N ago&amp;#10;    if len(sequence) &gt; n and curr_stim == sequence[-n]:&amp;#10;        remaining = [l for l in all_stimuli if l != sequence[-n]]&amp;#10;        curr_stim = random.choice(remaining)&amp;#10;    is_target = False&amp;#10;    &amp;#10;sequence.append(curr_stim)&amp;#10;thisExp.addData('stim', curr_stim)&amp;#10;thisExp.addData('is_target', is_target)" valType="extendedCode" updates="constant" name="Begin Routine"/>
